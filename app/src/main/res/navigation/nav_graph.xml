<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_homeBottomNavigationFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"
            app:destination="@id/homeBottomNavigationFragment" />
    </fragment>

    <fragment
        android:id="@+id/calendarFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar"/>

    <fragment
        android:id="@+id/moreFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" />

    <fragment
        android:id="@+id/moodEntryFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.addentry.AddMoodEntryFragment"
        android:label="fragment_add_mood_entry"
        tools:layout="@layout/fragment_mood_entry">
        <action
            android:id="@+id/action_moodEntryFragment_to_taskEntryFragment"
            app:destination="@id/taskEntryFragment" />
        <action
            android:id="@+id/action_moodEntryFragment_to_editMoodFragment"
            app:destination="@id/editMoodFragment" />
    </fragment>

    <fragment
        android:id="@+id/taskEntryFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.addentry.AddTaskEntryFragment"
        android:label="fragment_add_task_entry"
        tools:layout="@layout/fragment_task_entry">
        <action
            android:id="@+id/action_taskEntryFragment_to_taskCategoriesFragment"
            app:destination="@id/taskCategoriesFragment" />
    </fragment>

    <fragment
        android:id="@+id/editMoodFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.upsert.MoodIconsFragment"
        android:label="fragment_edit_mood"
        tools:layout="@layout/fragment_show_list">
        <action
            android:id="@+id/action_editMoodFragment_to_upsertMoodIconFragment"
            app:destination="@id/upsertMoodIconFragment" />
    </fragment>

    <fragment
        android:id="@+id/upsertMoodIconFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.upsert.UpsertMoodIconFragment"
        android:label="fragment_upsert_mood_icon"
        tools:layout="@layout/fragment_upsert_mood_icon" />

    <fragment
        android:id="@+id/taskItemsFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.upsert.TaskItemsFragment"
        android:label="TaskItemsFragment">
        <action
            android:id="@+id/action_taskItemsFragment_to_upsertTaskIconFragment"
            app:destination="@id/upsertTaskIconFragment" />
    </fragment>

    <fragment
        android:id="@+id/upsertTaskIconFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.upsert.UpsertTaskIconFragment"
        android:label="UpsertTaskIconFragment" />

    <fragment
        android:id="@+id/singleDateUserEntriesFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.displayentry.SingleDateUserEntriesFragment"
        android:label="SingleDateUserEntriesFragment" />

    <fragment
        android:id="@+id/allUserEntriesFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.displayentry.AllUserEntriesFragment"
        android:label="AllUserEntriesFragment" />
    <fragment
        android:id="@+id/homeBottomNavigationFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.home.HomeBottomNavigationFragment"
        android:label="fragment_home_bottom_navigation"
        tools:layout="@layout/fragment_home_bottom_navigation" >
        <action
            android:id="@+id/action_homeBottomNavigationFragment_to_moodEntryFragment"
            app:destination="@id/moodEntryFragment" />
    </fragment>
    <fragment
        android:id="@+id/taskCategoriesFragment"
        android:name="lostankit7.droid.moodtracker.ui.fragment.upsert.TaskCategoriesFragment"
        android:label="TaskCategoriesFragment" >
        <action
            android:id="@+id/action_taskCategoriesFragment_to_taskItemsFragment"
            app:destination="@id/taskItemsFragment" />
    </fragment>
</navigation>