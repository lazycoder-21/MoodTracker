<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="lostankit7.droid.moodtracker.ui.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_userEntriesFragment"
            app:destination="@id/userEntriesFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/userEntriesFragment"
        android:name="lostankit7.droid.moodtracker.ui.userEntries.UserEntriesFragment"
        android:label="fragment_entries"
        tools:layout="@layout/fragment_user_entries" />
    <fragment
        android:id="@+id/calendarFragment"
        android:name="lostankit7.droid.moodtracker.ui.calendar.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" />
    <fragment
        android:id="@+id/moreFragment"
        android:name="lostankit7.droid.moodtracker.ui.more.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" />
    <fragment
        android:id="@+id/moodEntryFragment"
        android:name="lostankit7.droid.moodtracker.ui.entry.mood.addEntry.MoodEntryFragment"
        android:label="fragment_add_mood_entry"
        tools:layout="@layout/fragment_mood_entry">
        <action
            android:id="@+id/action_moodEntryFragment_to_taskEntryFragment"
            app:destination="@id/taskEntryFragment" />
        <action
            android:id="@+id/action_moodEntryFragment_to_editMoodFragment"
            app:destination="@id/editMoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/taskEntryFragment"
        android:name="lostankit7.droid.moodtracker.ui.entry.task.addTask.TaskEntryFragment"
        android:label="fragment_add_task_entry"
        tools:layout="@layout/fragment_task_entry">
        <argument
            android:name="@string/arg_to_addNewTaskFrag"
            android:defaultValue="@null"
            app:argType="lostankit7.droid.moodtracker.model.MoodEntry"
            app:nullable="true" />
        <action
            android:id="@+id/action_taskEntryFragment_to_entriesFragment"
            app:destination="@id/userEntriesFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_taskEntryFragment_to_editTaskFragment"
            app:destination="@id/taskCategoriesFragment" />
    </fragment>
    <fragment
        android:id="@+id/editMoodFragment"
        android:name="lostankit7.droid.moodtracker.ui.entry.mood.editMood.MoodListFragment"
        android:label="fragment_edit_mood"
        tools:layout="@layout/fragment_show_list">
        <action
            android:id="@+id/action_editMoodFragment_to_upsertMoodIconFragment"
            app:destination="@id/upsertMoodIconFragment"
            app:enterAnim="@anim/anim_bottom_to_top"
            app:popExitAnim="@anim/anim_top_to_bottom" />
    </fragment>
    <fragment
        android:id="@+id/upsertMoodIconFragment"
        android:name="lostankit7.droid.moodtracker.ui.entry.mood.editMood.UpsertMoodIconFragment"
        android:label="fragment_upsert_mood_icon"
        tools:layout="@layout/fragment_upsert_mood_icon">
        <argument
            android:name="@string/arg_to_upsertMoodFrag"
            android:defaultValue="@null"
            app:argType="lostankit7.droid.moodtracker.data.database.entities.MoodIcon"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/taskCategoriesFragment"
        android:name="lostankit7.droid.moodtracker.ui.entry.task.editTask.TaskCategoriesFragment"
        android:label="EditTaskFragment">
        <argument
            android:name="@string/arg_to_taskItemsFrag"
            app:argType="string" />
        <action
            android:id="@+id/action_taskCategoriesFragment_to_taskItemsFragment"
            app:destination="@id/taskItemsFragment" />
    </fragment>
    <fragment
        android:id="@+id/taskItemsFragment"
        android:name="lostankit7.droid.moodtracker.ui.entry.task.editTask.TaskItemsFragment"
        android:label="TaskItemsFragment" />
</navigation>